		ApplySpell(Myself,WIZARD_STONE_SKIN)
		SetInterrupt(TRUE)
END

IF
	Global("RR#Active","LOCALS",0)
	OR(3)
		AttackedBy([ANYONE],DEFAULT)
		TookDamage()
		Heard([ANYONE],124)
THEN
	RESPONSE #100
		SetGlobal("RR#Active","LOCALS",1)
		SetGlobalTimer("RR#Rest","LOCALS",2400) // 8 hours
		Shout(124)
		Continue()
END

IF
	Global("RR#PSCRL","LOCALS",0)
THEN
	RESPONSE #20
		SetGlobal("RR#PSCRL","LOCALS",1)
		GiveItemCreate("SCRL5B",Myself,1,1,0) // Defensive Harmony
		GiveItemCreate("SCRL1Y",Myself,1,1,0) // Improved Invisibility
		Continue()
