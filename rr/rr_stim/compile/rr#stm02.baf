		SetGlobal("RR#PSCRL","LOCALS",1)
		GiveItemCreate("SCRL58",Myself,1,1,0) // Free Action
		GiveItemCreate("SCRL1Y",Myself,1,1,0) // Improved Invisibility
		Continue()
	RESPONSE #20
		SetGlobal("RR#PSCRL","LOCALS",1)
		GiveItemCreate("SCRL5F",Myself,1,1,0) // Chaotic Commands
		GiveItemCreate("SCRL2H",Myself,1,1,0) // Shadow Door
		Continue()
